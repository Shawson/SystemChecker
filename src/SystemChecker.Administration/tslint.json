{
  "extends": "tslint:recommended",
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": [
      false
    ],
    "max-line-length": [
      true,
      200
    ],
    "arrow-parens": false,
    "radix": false,
    "no-angle-bracket-type-assertion": false,
    "indent": [
      false
    ],
    "whitespace": [
      false
    ]
  }
}